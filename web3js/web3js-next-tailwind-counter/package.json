{
  "name": "web3js-next-tailwind-counter",
  "displayName": "Web3js Counter",
  "usecase": "Legacy",
  "description": "Next.js, Tailwind, @trezoa/web3.js, Wallet Adapter, TrezoaAnchor Counter program",
  "keywords": [
    "trezoaanchor-counter",
    "nextjs",
    "react",
    "trezoa-web3js",
    "tailwind",
    "typescript",
    "wallet-adapter"
  ],
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "trezoaanchor": "cd trezoaanchor && trezoaanchor",
    "trezoaanchor-build": "cd trezoaanchor && trezoaanchor build",
    "trezoaanchor-localnet": "cd trezoaanchor && trezoaanchor localnet",
    "trezoaanchor-test": "cd trezoaanchor && trezoaanchor test",
    "build": "next build",
    "ci": "npm run build && npm run lint && npm run format:check",
    "dev": "next dev --turbopack",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint",
    "start": "next start"
  },
  "dependencies": {
    "@trezoa-xyz/trezoaanchor": "^0.31.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@trezoa/tpl-token": "0.4.14",
    "@trezoa/wallet-adapter-base": "0.9.27",
    "@trezoa/wallet-adapter-react": "0.15.39",
    "@trezoa/wallet-adapter-react-ui": "0.9.39",
    "@trezoa/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.89.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jotai": "^2.14.0",
    "lucide-react": "^0.544.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bn.js": "^5.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.35.0",
    "eslint-config-next": "16.0.10",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.3",
    "typescript": "^5.9.2"
  },
  "create-trezoa-dapp": {
    "rename": {
      "scaffold": {
        "to": "{{name}}",
        "paths": [
          "src"
        ]
      }
    }
  }
}
